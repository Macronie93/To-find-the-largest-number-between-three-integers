// Find the largest number
#include <stdio.h>
int main( )
{
 int a,b,c;
 printf("Enter the value's     ");
 scanf("%d %d %d",&a,&b,&c );

 if (a>b && a>c )
 {
 printf("A is greater than B and C");
 } else if(b>a && b>c) 
 {
 	printf("B is greater than A and C");
 }
 else {
 	printf(" C is greater than A and B");
}
}
